
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            Student Id
        </th>
        <th>
            Name
        </th>
        <th>
            Address
        </th>
        <th>

        </th>
    </tr>

    @foreach (var stu in ViewBag.Data)
    {
        <tr>
            <td>
               @stu.StudentId
            </td>
            <td>
                @stu.Name
                
            </td>
            <td>
               @stu.Address
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = @stu.StudentId }) | 
                @Html.ActionLink("Delete", "Delete", new { id = @stu.StudentId }) 
            </td>
        </tr>
    }

</table>